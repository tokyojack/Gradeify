<!DOCTYPE html>
<head>
   <meta charset="utf-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
   <title>heavyTable.js : jQuery plugin</title>
   <meta name="description" content="">
   <meta name="viewport" content="width=device-width">
   <script src="/js/utils/jquery-3.2.1.slim.min.js" type="text/javascript"></script>
   <!--<script src="/js/utils/jquery.heavyTable.js" type="text/javascript"></script>-->
   <link rel="stylesheet" type="text/css" href="/css/utils/normalize.css">
   <link rel="stylesheet" type="text/css" href="/css/class.css">
</head>
<body>
   <button>Save</button>
   <br />
   <a href="/home">Home</a>
   
   <div class='main'>
      <table class="heavyTable">
         <thead>
            <tr>
               <th/>
               
                  <% for (var i=0; i < teachers.length; i++){ %>
               <th />
                  <div><%= teachers[i].firstName  %> <%= teachers[i].lastName  %> </div>
               <% } %>
            </tr>
         </thead>
         <tbody>
            <% for (var i=0; i < classes.length; i++){ %>
            <tr>
               <th><%= classes[i].className %></th>
               
               
               
            </tr>
            <% } %>
         </tbody>
      </table>
   </div>
   <script src="/js/class.js" type="text/javascript" ></script>
</body>
</html>

<!-- Teachers on top - Classes on right -->